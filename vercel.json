{
	"version": 2,
	"builds": [
		{
			"src": "/apps/api/src/server.ts",
			"use": "@vercel/node"
		},
		{
			"src": "/apps/web/.next/*",
			"use": "@now/static"
		}
	],
	"routes": [
		{
			"src": "/(.*)",
			"dest": "/apps/api/src/server.ts",
			"methods": ["GET", "POST", "PUT", "DELETE", "PATCH", "OPTIONS"],
			"headers": {
				"Access-Control-Allow-Origin": "*"
			}
		}
	]
}
